
page{
  height: 100%;
}
.scroll_view{
  height: 100%;
}

.scroll_wapper{
  padding: 15rpx;
}
.swiper_image{
  border-radius: 15rpx;
  height: 100%;
  width: 100%;
}
.ablum_box_list{
  display: flex;
  flex-wrap: wrap;
}
.ablum_item{
  width: calc( 100% / 3 );
  padding: 15rpx;
  box-sizing: border-box;
}
.ablum_item image{
  width: 100%;
  height: 220rpx;
  border-radius: 10rpx;
}
.ablumcontainer{
  padding: 15rpx;
}
.ablum_box_title{
  font-size: 40rpx;
  font-weight: bold;
  margin: 10px 0 ;
}
.album_name{
  font-size: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.price{
  color: #999999;
  font-size: 28rpx;
}