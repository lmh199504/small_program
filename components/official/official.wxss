/* components/official/official.wxss */
.title{
  color:rgb(0, 0, 0);
  font-size: 30rpx;
  font-weight: bold;
  margin-left: 3%;
}
.music_list_container{
  display: flex;
  flex-wrap: wrap;
  justify-content:space-between;
  width: 94%;
  padding:1% 3%;
}
.music_list_item{
  width: 32%;
  height: 120px;
  border-radius: 10px;
  margin-top: 10px;
  text-align: center;
}
.cover_img{
  width: 100%;
  height: 100px;
  border-radius: 10px;
}
.list_title{
  font-size: 25rpx;
  color:rgb(92, 94, 93);
  overflow: hidden;
  white-space: nowrap;
  text-overflow:ellipsis;
  width: 100%;
  display: block;
}